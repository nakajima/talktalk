struct Array {
	var _storage: pointer
	var count: int
	var capacity: int

	init() {
		self.count = 0
		self.capacity = 4
		self._storage = _allocate(4)
	}

	func append(item) {
		self.count = self.count + 1
		_storePtr(self._storage, item)
	}

	func at(index) {
		_deref(self._storage + index)
	}

	func resize() {
		newCapacity = self.capacity * 2
		newStorage = _allocate(newCapacity)


	}
}
